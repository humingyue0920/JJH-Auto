<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tester.model">
    
    <!--获取登录用户-->
    <select id="loginTest" parameterType="Integer" resultType="com.tester.model.LoginCase">
        select * from caseuser where id = #{id};
    </select>
    
    <!--获取代金券类型-->
    <select id="VoucherQueryTest" parameterType="Integer" resultType="com.tester.model.VoucherQueryCase">
        select * from casevoucherquery where id = #{id};
    </select>

</mapper>